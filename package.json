{
  "name": "txt2bas",
  "version": "1.3.2",
  "description": "Converts BASIC to Text and back again",
  "main": "dist/index.js",
  "module": "./index.js",
  "prettier": {
    "singleQuote": true
  },
  "bin": {
    "txt2bas": "dist/cli/index.js",
    "bas2txt": "dist/cli/index.js"
  },
  "scripts": {
    "build": "rm -rf dist; rollup --config && chmod 744 dist/cli/index.js",
    "dev": "npm run build; rollup --config --watch",
    "prepare": "npm run build",
    "test": "tap __tests__/*.test.js --no-coverage-report --watch"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "spectrum",
    "cli",
    "zx",
    "specnext",
    "nextbasic",
    "txt2bas",
    "basic"
  ],
  "author": "Remy Sharp",
  "license": "MIT",
  "dependencies": {
    "@remy/unpack": "^2.1.3"
  },
  "devDependencies": {
    "tap": "^14.10.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remy/txt2bas.git"
  },
  "bugs": {
    "url": "https://github.com/remy/txt2bas/issues"
  },
  "homepage": "https://github.com/remy/txt2bas#readme"
}
